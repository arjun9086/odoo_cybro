<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="action_rent_report" model="ir.actions.report">
        <field name="name">Rent Report</field>
        <field name="model">property.rental</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">property.report_rent_report_details</field>
        <field name="report_file">property.report_rent_report_details</field>
        <field name="paperformat_id" ref="property.paperformat_property_report"/>
        <field name="binding_model_id" ref="property.model_property_rental"/>
        <field name="binding_type">report</field>
    </record>
    <record id="paperformat_property_report" model="report.paperformat">
        <field name="name">A3</field>
        <field name="default" eval="True"/>
        <field name="format">A3</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">32</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">32</field>
        <field name="dpi">170</field>
        <field name="css_margins" eval="True"/>
    </record>
</odoo>